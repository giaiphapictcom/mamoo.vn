<body>
    <header class="container">
        <div class="left-container">
            <i class="fa fa-align-left js-toggle-menu"></i>
            <!-- Kiểm tra trường hợp khi đang ở trang nhà cung cấp -->
            <a class="head-title-img js-view-vendors pointer">
                <img src="/Content/Images/logo.png">
            </a><span class="fa fa-map-marker"></span><span class="head-sub">Hà Nội</span>
        </div>
        <div class="right-container">
            <i class="fa fa-search" id="js-btn-search-global"></i><i class="fa fa-shopping-cart js-toggle-cart"><span class="js-notify-cart count-product">(0)</span>        </i>
            <!-- <img class="cart-notify-mobile" src="//img.disieuthi.vn/styles/mobile/4038/image/cart-notify.gif"> -->
        </div>
        <div class="cart-notify-mobile">
            <div class="container-cart">
                <div class="main">
                    <div class="bg"></div>
                    <span class="text">Đặt hàng ngay</span>
                </div>
                <i class="fa fa-caret-up"></i><i class="fa fa-shopping-cart"></i>
            </div>
        </div>
    </header>
    <section id="global-search" class="container">
        <div class="m-search-page-header">
            <div class="group-search">
                <form id="js-frm-search-global" class="" name="frm-srch" action="/search/">
                    <i id="js-btn-search-gb" class="fa fa-search"></i>
                    <input id="id-search" type="hidden" name="vendor" value="">
                    <input class="js-input-search" type="text" autocomplete="off" value="" placeholder="Nhập từ khóa" name="q">
                    <i id="js-btn-del-gb" class="fa fa-times"></i>
                    <div class="clear"></div>
                </form>
            </div>
            <span id="js-btn-cancel-search-gb" class="cancel-button">hủy</span>
        </div>
        <div class="m-search-page-body"></div>
    </section>
    <section class="menu-category-product js-menu-panel">
        @Html.Action("MobileMenu", "Widget")
    </section>
    <div class="dr-shopping-cart-page">
        <div class="menu-overlay js-toggle-cart"></div>
        <div class="shopping-cart-page-header">
            <i class="fa fa-angle-left js-toggle-cart"></i>
            <label for="">Giỏ hàng</label>
            <span class="product-count js-notify-cart">(0)</span>
        </div>
        <div class="shopping-cart-page-body is-mn">
            <div class="empty-content">Vui lòng cho sản phẩm vào giỏ hàng        </div>
        </div>
        <div class="shopping-cart-page-footer">
            <div class="total-pay">
                <label>Tổng thanh toán</label>
                <span class="m-symbol-money m-red-txt">đ</span>            <span id="div_gio_hang_tong_tien" class="is-total-money-pop m-red-txt">0</span>
            </div>
            <div class="order">
                <div class="shopping-cart-button cart-saving-button">lưu giỏ</div>
                <div class="shopping-cart-button order-button js-purchase">đặt hàng</div>
            </div>
        </div>
    </div>
    @RenderBody()
    <style type="text/css">
        #js-support-backup {
