@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>Admin 365 - @ViewBag.Title</title>
    <meta name="description" content="" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="/Content/Admin/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="/Content/Admin/assets/css/bootstrap-responsive.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="/Content/Admin/assets/css/font-awesome.min.css" />
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Open+Sans:400,300" />
    <link rel="stylesheet" href="/Content/Admin/assets/css/ace.min.css" />
    <link rel="stylesheet" href="/Content/Admin/assets/css/ace-responsive.min.css" />
    <link rel="stylesheet" href="/Content/Admin/assets/css/ace-skins.min.css" />
    <link rel="stylesheet" href="/Content/Admin/assets/css/site.css" />
    <link rel="stylesheet" href="/Content/Admin/Styles/v308admin.css?version=3.0" />
    <script src="/Content/Admin/assets/js/jquery-1.10.2.min.js"></script>
    <script src="/Content/Admin/assets/js/bootstrap.min.js"></script>
    <script src="/Content/Admin/assets/js/ace-elements.min.js"></script>
    <script src="/Content/Admin/assets/js/ace.min.js"></script>
    <script src="/Content/Admin/assets/js/script.js"></script>
    <script src="/Content/Admin/Scripts/v308admin.js"></script>
    <script src="/Content/Admin/Scripts/v308news.js"></script>
    <script src="/Content/Admin/Scripts/v308newsgroup.js?v=2.0"></script>
    <script src="/Content/Admin/Scripts/v308imagesgroup.js"></script>
    <script src="/Content/Admin/Scripts/v308images.js"></script>
    <script src="/Content/Admin/Scripts/v308sanphamnhaphanphoi.js"></script>
    <script src="/Content/Admin/Scripts/v308sanphamnhasanxuat.js"></script>
    <script src="/Content/Admin/Scripts/v308sanphamloaisanpham.js"></script>
    <script src="/Content/Admin/Scripts/v308sanpham.js"></script>
    <script src="/Content/Admin/Scripts/v308supporttype.js"></script>
    <script src="/Content/Admin/Scripts/v308support.js"></script>
    <script src="/Content/Admin/Scripts/v308loaifile.js?v=@V308CMS.Common.AppSetting.Version"></script>
    <script src="/Content/Admin/Scripts/v308file.js?v=@V308CMS.Common.AppSetting.Version"></script>
    <script src="/Content/Admin/Scripts/v308order.js"></script>
    <script src="/Content/Admin/Scripts/v308adminaccount.js"></script>
    <script src="/Content/Admin/Scripts/v308account.js"></script>
    <script src="/Content/Admin/Scripts/v308market.js?v=2.0"></script>
    <script src="../../Content/Admin/Plugins/ckeditor/ckeditor.js" type="text/javascript"></script>
    <script src="../../Content/Admin/Plugins/ckfinder/ckfinder.js" type="text/javascript"></script>
     <script type="text/javascript">
         $(document).ready(function () {
             $(".chk").click(function () {
                 if ($(this).attr('checked') == 'checked')
                     $(this).removeAttr('checked')
                 else
                     $(this).attr('checked', 'checked')
             });
         });
    </script>
    @RenderSection("Header", false)
</head>
<body>
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
                <a href="#" class="brand"><small></small></a>
                <!--/.brand-->
                <ul class="nav ace-nav pull-right">
                    <li class="light-blue"><a data-toggle="dropdown" href="#" class="dropdown-toggle">
                        <img class="nav-user-photo" src="/Content/Admin/Images/avatar.png" alt="Avatar">
                        <span class="user-info"><small>Welcome,</small> Công ty TNHH công nghệ</span>
                        <i class="icon-caret-down"></i></a>
                        <ul class="user-menu pull-right dropdown-menu dropdown-yellow dropdown-caret dropdown-closer">
                           @* <li><a href="javascript:void(0);" title="Cài đặt"><i class="icon-cog"></i>Cài đặt</a> </li>*@
                            <li><a href="/Admin/AccountInfo/" title="Thông tin tài khoản"><i class="icon-user"></i>Thông tin tài khoản</a> </li>
                            <li class="divider"></li>
                            <li><a href="/Admin/LogOut" title="Thoát"><i class="icon-off"></i>Thoát</a>
                            </li>
                        </ul>
                    </li>
                </ul>
                <!--/.ace-nav-->
            </div>
            <!--/.container-fluid-->
        </div>
        <!--/.navbar-inner-->
    </div>
    <div class="main-container container-fluid">
        <a class="menu-toggler" id="menu-toggler" href="#"><span class="menu-text"></span>
        </a>
        <div class="sidebar" id="sidebar">
            <div class="sidebar-shortcuts" id="sidebar-shortcuts">
                <div class="sidebar-shortcuts-large" id="sidebar-shortcuts-large">
                    <button class="btn btn-small btn-success">
                        <i class="icon-signal"></i>
                    </button>
                    <button class="btn btn-small btn-info">
                        <i class="icon-pencil"></i>
                    </button>
                    <button class="btn btn-small btn-warning">
                        <i class="icon-group"></i>
                    </button>
                    <button class="btn btn-small btn-danger">
                        <i class="icon-cogs"></i>
                    </button>
                </div>
                <div class="sidebar-shortcuts-mini" id="sidebar-shortcuts-mini">
                    <span class="btn btn-success"></span><span class="btn btn-info"></span><span class="btn btn-warning"></span><span class="btn btn-danger"></span>
                </div>
            </div>
            <!--#sidebar-shortcuts-->
            <ul class="nav nav-list">
                <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(0, ViewBag.MenuValue))><a href="/Admin/Index"><i class="icon-dashboard"></i><span class="menu-text">Bảng điều khiển</span></a></li>
                <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(1, ViewBag.MenuValue))><a href="javascript:void(0);" class="dropdown-toggle"><i
                    class="icon-desktop"></i><span class="menu-text">Sản Phẩm</span><b class="arrow icon-angle-down"></b></a><ul
                        @Html.Raw(@V308CMS.Common.Ultility.CheckSubMenu(1, ViewBag.MenuValue)) class="submenu">
                          <li><a href="/Admin/SanPham?pPage=1&pType=0" title="Sản phẩm"><i class="icon-double-angle-right"></i>Sản Phẩm</a></li>
                        <li><a href="/Admin/SanPhamLoaiSanPham?pPage=1&pType=0" title="Thể loại sản phẩm"><i class="icon-double-angle-right"></i>Loại Sản Phẩm</a></li>
                     
                       @*  <li><a href="/Admin/SanPhamNhaSanXuat/"
                            title="Hãng sản xuất"><i class="icon-double-angle-right"></i>Nhà sản xuất</a></li>
                        <li><a href="/Admin/SanPhamNhaPhanPhoi/" title="Nhà phân phối"><i
                            class="icon-double-angle-right"></i>Nhà phân phối</a></li>*@
                    </ul>
                </li>
                 <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(10, ViewBag.MenuValue))><a href="javascript:void(0);" class="dropdown-toggle"><i class=" icon-laptop"></i><span
                    class="menu-text">Cửa hàng</span><b class="arrow icon-angle-down"></b></a><ul @Html.Raw(@V308CMS.Common.Ultility.CheckSubMenu(10, ViewBag.MenuValue)) class="submenu">
                        <li><a href="/Admin/Market/" title="Market"><i class="icon-double-angle-right"></i>Danh sách cửa hàng</a></li>
                        <li><a href="/Admin/TheLoaiTinTuc/" title="Thể loại tin tức"><i class="icon-double-angle-right"></i>Thêm loại sản phẩm</a></li>
                    </ul>
                </li>
                <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(2, ViewBag.MenuValue))><a href="javascript:void(0);" class="dropdown-toggle"><i class=" icon-laptop"></i><span
                    class="menu-text">Danh mục -Tin Tức</span><b class="arrow icon-angle-down"></b></a><ul @Html.Raw(@V308CMS.Common.Ultility.CheckSubMenu(2, ViewBag.MenuValue)) class="submenu">
                        <li><a href="/Admin/TinTuc/" title="Tin tức"><i class="icon-double-angle-right"></i>Tin
                            tức</a></li>
                        <li><a href="/Admin/TheLoaiTinTuc/" title="Thể loại tin tức"><i class="icon-double-angle-right"></i>Danh mục</a></li>
                    </ul>
                </li>
                <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(3, ViewBag.MenuValue))><a href="javascript:void(0);" class="dropdown-toggle"><i class="icon-group"></i><span
                    class="menu-text">Khách Hàng</span><b class="arrow icon-angle-down"></b></a><ul @Html.Raw(@V308CMS.Common.Ultility.CheckSubMenu(3, ViewBag.MenuValue)) class="submenu">
                        <li><a href="/Admin/Order/" title="Đơn hàng"><i class="icon-double-angle-right"></i>Đơn hàng</a></li>
                        <li><a href="/Admin/ChucNang/" title="Góp ý sản phẩm"><i class="icon-double-angle-right"></i>Góp ý sản phẩm</a></li>
                    </ul>
                </li>
                <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(4, ViewBag.MenuValue))><a href="javascript:void(0);" class="dropdown-toggle"><i class="icon-picture"></i><span
                    class="menu-text">Banner</span><b class="arrow icon-angle-down"></b></a><ul @Html.Raw(@V308CMS.Common.Ultility.CheckSubMenu(4, ViewBag.MenuValue)) class="submenu">
                        <li><a href="/Admin/TheLoaiAnh/" title="Loại hình ảnh"><i class="icon-double-angle-right"></i>Loại Banner</a></li>
                        <li><a href="/Admin/Anh/" title="Hình ảnh"><i class="icon-double-angle-right"></i>Banner</a></li>
                    </ul>
                </li>
                <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(5, ViewBag.MenuValue))><a href="javascript:void(0);" class="dropdown-toggle"><i class="icon-file"></i><span
                    class="menu-text">Dữ liệu hệ thống</span><b class="arrow icon-angle-down"></b></a><ul @Html.Raw(@V308CMS.Common.Ultility.CheckSubMenu(5, ViewBag.MenuValue))
                        class="submenu">
                        <li><a href="/Admin/TheLoaiFile/" title="Thể loại file"><i class="icon-double-angle-right"></i>Loại dữ liệu</a></li>
                        <li><a href="/Admin/File/" title="File"><i class="icon-double-angle-right"></i>Danh sách dữ liệu</a></li>
                    </ul>
                </li>
                <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(6, ViewBag.MenuValue))><a href="javascript:void(0);" class="dropdown-toggle"><i class="icon-user"></i><span
                    class="menu-text">Tài Khoản</span><b class="arrow icon-angle-down"></b></a><ul @Html.Raw(@V308CMS.Common.Ultility.CheckSubMenu(6, ViewBag.MenuValue)) class="submenu">
                        <li><a href="/Admin/AdminAccount/" title="Tài khoản quản trị">
                            <i class="icon-double-angle-right"></i>Tài khoản quản trị</a></li>
                        <li><a href="/Admin/Account/" title="Tài khoản khách hàng"><i class="icon-double-angle-right"></i>Tài khoản khách hàng</a></li>
                        <li><a href="/Admin/AccountInfo/" title="Thông tin tài khoản"><i class="icon-double-angle-right"></i>Thông tin tài khoản</a></li>
                        
                    </ul>
                </li>
                <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(7, ViewBag.MenuValue))><a href="javascript:void(0);" class="dropdown-toggle"><i class="icon-windows"></i><span
                    class="menu-text">Hệ Thống</span><b class="arrow icon-angle-down"></b></a><ul @Html.Raw(@V308CMS.Common.Ultility.CheckSubMenu(7, ViewBag.MenuValue)) class="submenu">
                        <li><a href="/Admin/HoTro/" title="Hỗ trợ trực tuyến"><i class="icon-double-angle-right"></i>Hỗ trợ trực tuyến</a></li>
                        <li><a href="/Admin/HoTroLoaiHoTro/" title="Loại hỗ trợ trực tuyến">
                            <i class="icon-double-angle-right"></i>Loại hỗ trợ trực tuyến</a></li>
                    </ul>
                </li>
                <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(8, ViewBag.MenuValue))><a href="javascript:void(0);" class="dropdown-toggle"><i class="icon-trash"></i><span
                    class="menu-text">Thùng Rác</span><b class="arrow icon-angle-down"></b></a><ul @Html.Raw(@V308CMS.Common.Ultility.CheckSubMenu(8, ViewBag.MenuValue)) class="submenu">
                        <li><a href="/Admin/ChucNang/" title="Thùng rác"><i class="icon-double-angle-right"></i>Thùng rác</a></li>
                    </ul>
                </li>
                <li @Html.Raw(@V308CMS.Common.Ultility.CheckMenu(9, ViewBag.MenuValue))><a href="javascript:void(0);" class="dropdown-toggle"><i class="icon-bookmark"></i><span
                    class="menu-text">Quyền truy cập</span><b class="arrow icon-angle-down"></b></a><ul @Html.Raw(@V308CMS.Common.Ultility.CheckSubMenu(9, ViewBag.MenuValue))
                        class="submenu">
                        <li><a href="/Admin/ChucNang/" title="Page"><i class="icon-double-angle-right"></i>Page</a></li>
                        <li>
                            <a href="/Admin/ChucNang/" title="Role"><i class="icon-double-angle-right"></i>Role</a></li>
                        
                    </ul>
                </li>
            </ul>
            <!--/.nav-list-->
            <div class="sidebar-collapse" id="sidebar-collapse">
                <i class="icon-double-angle-left"></i>
            </div>
        </div>
        <div class="main-content">
            <div class="breadcrumbs" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li><i class="icon-home home-icon"></i><a href="/Admin">Home</a> <span class="divider"><i
                        class="icon-angle-right arrow-icon"></i></span></li>
                    @Html.Raw(@ViewBag.LinkBack)
                </ul>
                <!--.breadcrumb-->
                <div class="nav-search" id="nav-search">
                    <div class="form-search">
                    </div>
                </div>
                <!--#nav-search-->
            </div>
            <div class="page-content">
                <div class="row-fluid">
                    <div class="span12">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
        <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-small btn-inverse"><i
            class="icon-double-angle-up icon-only bigger-110"></i></a>
    </div>
    <div class="pct" id="popupId" style="display: none;">
        <div class="mpopup">
            <div class="mpopup_header">
                <div class="mpopup_title">Thông báo</div>
                <div class="mpopup_close" onclick="javascript:ClosePopup();"></div>
            </div>
            <div class="mpopup_content">Chào bạn !</div>
            <div class="mcontrols"><a class="controlok" href="javascript:ClosePopup();">OK</a><a class="controlno" href="javascript:ClosePopup();">NO</a></div>
        </div>
    </div>
    <div id="wait">
        <div class="innerWait">
        </div>
    </div>
</body>
</html>
